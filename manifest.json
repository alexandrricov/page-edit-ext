{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon1.png",
    "default_title": "Click for edit."
  },

  "permissions": [
    "activeTab"
  ]
}
